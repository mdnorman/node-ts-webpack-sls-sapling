{
  "author": "Michael D. Norman",
  "name": "CHANGEME",
  "description": "",
  "private": true,
  "license": "UNLICENSED",
  "version": "0.0.0",
  "homepage": "https://github.com/mdnorman/CHANGEME/wiki/Home",
  "repository": {
    "type": "git",
    "url": "https://github.com/mdnorman/CHANGEME"
  },
  "bugs": {
    "url": "https://github.com/mdnorman/CHANGEME/issues"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf out",
    "build": "webpack --mode production",
    "build:development": "webpack --mode development",
    "rebuild": "npm run clean && npm run build",
    "rebuild:development": "npm run clean && npm run build:development",
    "pretest": "tsc -p spec/tsconfig.json && webpack --mode production",
    "test": "jest",
    "test:integration": "npm run pretest && jasmine --config=spec/support/jasmine-integration.json",
    "posttest": "npm run lint",
    "lint": "eslint",
    "pretty": "pretty-quick",
    "server:development": "node dev-server.js",
    "deploy": "serverless deploy --verbose --stage test && serverless client deploy --verbose --no-confirm --no-delete-contents --stage test",
    "deploy:prod": "serverless deploy --verbose --stage prod && serverless client deploy --verbose --no-confirm --no-delete-contents --stage prod"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/node": "16.11.19",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "css-loader": "6.5.1",
    "eslint": "8.6.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "resolve-url-loader": "4.0.0",
    "sass-loader": "12.4.0",
    "serverless": "2.70.0",
    "serverless-finch": "2.8.0",
    "source-map-loader": "3.0.1",
    "style-loader": "3.3.1",
    "ts-jest": "27.1.2",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsutils": "3.21.0",
    "typescript": "4.5.4",
    "url-loader": "4.1.1",
    "webpack": "5.65.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.7.2"
  },
  "engines": {
    "npm": ">=7.20",
    "node": ">=16.5"
  }
}
